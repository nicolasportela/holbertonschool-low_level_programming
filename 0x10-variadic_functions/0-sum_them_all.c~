#include <stdio.h>
#include <stdarg.h>
#include "variadic_functions.h"

/**
 * sum_them_all - function
 * @n: variable name
 *
 * Return: void
 */

int sum_them_all(const unsigned int n, ...)
{
	va_list sum;
	unsigned int suma;
	unsigned int i;

	if (n == 0)
	{
		return (0);
	}
	va_start(sum, n);
	for (i = 0; i <= n; i++)
	{
		suma = suma + va_arg(sum, unsigned int);
	}
	va_end(sum);
	return (suma);
}
